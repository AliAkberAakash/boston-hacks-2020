<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.aliakberaakash.BostonHacks2020">

    <application
        android:name="com.aliakberaakash.BostonHacks2020.core.MyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.CutieHacksProject2020">
        <activity
            android:name="com.aliakberaakash.BostonHacks2020.ui.features.login.LoginActivity"
            android:theme="@style/Theme.AppCompat.Light.NoActionBar.FullScreen"
            android:configChanges="orientation"
            android:screenOrientation="portrait"
            >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.aliakberaakash.BostonHacks2020.MainActivity"
            android:configChanges="orientation"
            android:screenOrientation="portrait"
            >
        </activity>
    </application>

</manifest>